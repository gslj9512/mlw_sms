{
    "name": "mlw/sms",
    "description": "MLW SMS library for integration with MlwSms SMS service.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "MlwSms\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "license": "MIT"
}
